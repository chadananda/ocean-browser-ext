{
  "name": "Ocean Search 2.0",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension popup ocean search",
  "icons": {
    "128": "/icons/ocean.png",
    "48": "/icons/ocean.png",
    "16": "/icons/ocean.png"
  },
  "browser_action": {
    "default_icon": "icons/ocean.png",
    "default_title": "Ocean Search",
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "http://*/*",
    "https://*/*",
    "file://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "lib/jquery.min.js",
        "content/helper.js",
        "content/content.js"
      ],
      "run_at": "document_end"
    },{
      "matches": [
        "https://www.google.com/"
      ],
      "js": [
        "lib/jquery.min.js",
        "content/helper.js",
        "content/google.js"
      ],
      "run_at":"document_end"
    },{
      "matches": [
        "https://*.wikipedia.org/wiki/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "content/helper.js",
        "content/wiki.js"
      ],
      "run_at": "document_end"
    },{
      "matches": [
        "http://bahai-library.com/"
      ],
      "js": [
        "lib/jquery.min.js",
        "content/helper.js",
        "content/bahai-lib.js"
      ],
      "run_at": "document_end"
    },{
      "matches": [
        "https://www.bahai.org/"
      ],
      "js": [
        "lib/jquery.min.js",
        "content/helper.js",
        "content/www-bahai.js"
      ],
      "run_at":"document_end"
    },{
      "matches": [
        "https://news.bahai.org/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "content/helper.js",
        "content/news-bahai.js"
      ],
      "run_at":"document_end"
    }
  ],
  "background": {
    "scripts": [
      "lib/jquery.min.js",
      "background.js"
    ],
    "persistent": false
  },
  "commands": {
    "toggle-feature-foo": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Toggle feature foo"
    }
  }
}